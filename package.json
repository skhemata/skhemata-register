    "skhemata-css": "git+ssh://git@ark.skhemata.com/skhemata-css",
    "skhemata-form": "git+ssh://git@ark.skhemata.com/skhemata-form"
  },
  "devDependencies": {
    "@types/node": "13.11.1",
